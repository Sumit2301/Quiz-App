{
    "version": 2,
    "builds": [
      {
        "src": "quiz_app/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "env": {
      "DJANGO_SECRET_KEY": "your-secret-key",
      "DATABASE_URL": "mysql://user:password@host:port/dbname"
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "quiz_app.wsgi"
      }
    ],
    "builds": [
      {
        "src": "build_files.sh",
        "use": "@vercel/bash"
      }
    ]
  }
  